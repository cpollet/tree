{% autoescape 'js' %}
{
    "name": "{{ name }}",
    "type": "{{ type }}"

    {% if (attributes|length > 0) %}
    ,
    "attributes": {
    {% for attr in attributes %}
        "{{ attr.key }}": "{{ attr.value }}"
        {% if (loop.last == false) %}
        ,
        {% endif %}
    {% endfor %}
    }
    {% endif %}

    {% if (children|length > 0) %}
        ,
        "children": [
            {{ join(children, ',') }}
        ]
    {% endif %}
}
{% endautoescape %}